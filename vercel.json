{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.1"
    }
  },
  "routes": [
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/livre(.*)",
      "dest": "/api/livre.php$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/index.php",
      "dest": "/api/index.php"
    }
  ]
}
